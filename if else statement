#include<stdio.h>

int main(){
int a,b;

printf("enter a number: ");
scanf("%d",&a);
printf("enter a number");
scanf("%d",&b);
if (a != b)
{
    printf("\nthe given number are not equal\n");
    if (a<b)
    {
        printf("\nthe given number a is greater than b\n");
    }
    else
    {
        printf("\nthe given number b is greater than a\n");
    }
}
    else
    {
      printf("\nthe given number are equal\n");
    }
    return 0;

}
